<h1>Artists</h1>
<hr class="style1">

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th>Album</th>
  </tr>

  <% albums = @artist.find_album %>
  <% for album in albums %>
  <tr>
    <td class="td2"><%= @artist.name %></td>
    <td class="td2"></td>
    <td class="td2"><%= album.name %></td>
  </tr>
  <% end %>

</table>


